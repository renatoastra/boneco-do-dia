{
	"dependencies": {
		"@paralleldrive/cuid2": "^2.2.2",
		"drizzle-orm": "^0.44.5",
		"drizzle-zod": "0.8.3",
		"pg": "^8.16.0",
		"zod": "^4.1.5"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@gebra/tsconfig": "workspace:*",
		"@types/node": "22.15.30",
		"dotenv-cli": "^8.0.0",
		"drizzle-kit": "^0.31.1"
	},
	"main": "./index.ts",
	"name": "@gebra/database",
	"scripts": {
		"generate": "drizzle-kit generate --config drizzle/drizzle.config.ts",
		"push": "dotenv -c -e ../../.env -- drizzle-kit push --config drizzle/drizzle.config.ts",
		"drop": "dotenv -c -e ../../.env -- drizzle-kit drop --config drizzle/drizzle.config.ts",
		"migrate": "dotenv -c -e ../../.env -- drizzle-kit migrate --config drizzle/drizzle.config.ts",
		"studio": "dotenv -c -e ../../.env -- drizzle-kit studio --config drizzle/drizzle.config.ts",
		"type-check": "tsc --noEmit"
	},
	"types": "./**/.tsx",
	"version": "0.0.0"
}
